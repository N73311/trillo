{
  "name": "trillo",
  "version": "1.0.0",
  "description": "Hotel booking application",
  "main": "index.js",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/style.css --watch",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "build": "npm run build:css",
    "compile:sass": "sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/icon-font.css css/style.comp.css",
    "prefix:css": "postcss css/style.comp.css -o css/style.prefix.css --use autoprefixer",
    "compress:css": "sass css/style.prefix.css css/style.css --style=compressed --no-source-map",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": {
    "name": "Zachariah Ayers",
    "email": "zach@meridianvega.com",
    "company": "Meridian Vega"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": "22.20.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "sass": "^1.93.2"
  }
}
